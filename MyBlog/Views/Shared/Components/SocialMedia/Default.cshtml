@using MyBlog.Models.ViewComponentModels
@model IEnumerable<SocialMediaView>

@if (Model != null)
{
    <ul class="list-inline text-center">
        @foreach (var item in Model ?? new List<SocialMediaView>())
        {
            <li class="list-inline-item">
                <a href="@item.SocialMediaUrl" target="@item?.Target">
                    <span class="fa-stack fa-lg">
                        <i class="fas fa-circle fa-stack-2x"></i>
                        <i class="@item?.Icon fa-stack-1x fa-inverse"></i>
                    </span>
                </a>
            </li>
        }
    </ul>
}
