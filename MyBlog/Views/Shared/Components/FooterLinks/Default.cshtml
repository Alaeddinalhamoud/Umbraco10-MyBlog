@using MyBlog.App_Code.Models.ViewComponentModels
@model IEnumerable<FooterLinkView>

@if (Model != null)
{
    <div class="text-center mb-2">
        @foreach (var item in Model ?? new List<FooterLinkView>())
        {
            <a href="@item.Url" title="@item.Name" target="@item?.Target">
                @item.Name @(item?.Name == Model?.Last()?.Name ? "" : " | ")
            </a>
        }      
    </div>
}
